<HTML>
  <HEAD>
  <TITLE>Voting Registration</TITLE>
  </HEAD>

  <BODY>
  <center>
  <h2>Enter your user id and password to vote</h2>
  </center>

  <%= form_tag :action => 'list' do %>

  <center>
  <table>
    <tr><td>Username: </td><td><%= text_field_tag :username%></td></tr>
    <tr><td>Password: </td><td><%= password_field_tag :pword%></td></tr>
  </table>
  <br />
  <%= submit_tag 'Login' %>
  </center>

  <% end %>

  </BODY>
  </HTML>
